{
  "name": "proc3",
  "strict": false,
  "schema": {
    "type": "object",
    "properties": {
      "$schema": {
        "type": "string",
        "const": "https://json-schema.axonivy.com/process/12.0.0/process.json"
      },
      "id": {
        "type": "string",
        "pattern": "^[A-F0-9]{16,}$",
        "minLength": 16,
        "maxLength": 16
      },
      "description": {
        "type": "string"
      },
      "kind": {
        "$ref": "#/$defs/ProcessKind"
      },
      "config" : {
        "$ref" : "#/$defs/RootProcessConfig"
      },
      "docs": {
        "$ref": "#/$defs/Map(String,String)"
      }
    },
    "additionalProperties": false,
    "required": [
      "$schema",
      "id",
      "config"
    ],
    "$defs": {
      "ProcessKind": {
        "type": "string",
        "enum": [
          "NORMAL",
          "WEB_SERVICE",
          "CALLABLE_SUB",
          "HTML_DIALOG"
        ]
      },
      "Map(String,String)": {
        "type": "object",
        "properties": {},
        "additionalProperties": {
          "type": "string"
        }
      },
      "RootPermissions" : {
        "type" : "object",
        "properties" : {
          "view" : {
            "$ref" : "#/$defs/RootView"
          }
        },
        "additionalProperties" : false
      },
      "RootProcessConfig" : {
        "type" : "object",
        "properties" : {
          "data" : {
            "type" : "string"
          },
          "permissions" : {
            "$ref" : "#/$defs/RootPermissions"
          },
          "wsAuth" : {
            "$ref" : "#/$defs/WsAuth"
          },
          "wsTypeName" : {
            "type" : "string"
          }
        },
        "required" : [ "data" ],
        "additionalProperties" : false
      },
      "RootView" : {
        "type" : "object",
        "properties" : {
          "allowed" : {
            "type" : "boolean"
          }
        },
        "additionalProperties" : false
      },
      "WsAuth" : {
        "type" : "string",
        "enum" : [ "NONE", "WS_SECURITY", "HTTP_BASIC" ]
      }
    }
  }
}